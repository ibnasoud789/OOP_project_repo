<AnchorPane id="AnchorPane" prefHeight="468.0" prefWidth="806.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="erin.MobileBankController">
   <children>
      <Label layoutX="65.0" layoutY="101.0" text="Select Your Mobile App:" textFill="#9a0000">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="combobox" layoutX="337.0" layoutY="99.0" onAction="#comboboxOnAction" prefWidth="150.0" />
      <Label layoutX="65.0" layoutY="152.0" text="Enter Your Mobile Number:" textFill="#9a0000">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="mobilenum" layoutX="336.0" layoutY="150.0" onAction="#mobilenumOnAction" />
      <Label layoutX="63.0" layoutY="207.0" text="Enter Your PIN Number:" textFill="#9a0000">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="pinnum" layoutX="336.0" layoutY="205.0" onAction="#pinnumOnAction" />
      <Button fx:id="submitbutton" layoutX="378.0" layoutY="295.0" mnemonicParsing="false" onAction="#submitbuttonOnAction" text="Submit" />
      <Button fx:id="backbutton" layoutX="37.0" layoutY="405.0" mnemonicParsing="false" onAction="#backbuttonOnAction" text="Back" />
