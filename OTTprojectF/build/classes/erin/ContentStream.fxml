<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="539.0" prefWidth="846.0" style="-fx-background-color: Black;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="ottprojectf.ContentStreamController">
   <children>
      <Label layoutX="299.0" layoutY="70.0" text="Stream Your Content" textFill="WHITE">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <Label layoutX="74.0" layoutY="160.0" text="Choose from files:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="chooseFile" layoutX="316.0" layoutY="161.0" mnemonicParsing="false" onAction="#choosefromPC" text="Choose from files." />
      <Label layoutX="74.0" layoutY="209.0" text="Privacy:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <ComboBox fx:id="selectPrivacy" layoutX="316.0" layoutY="210.0" prefWidth="150.0" />
      <Label layoutX="74.0" layoutY="254.0" text="Category:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <ComboBox fx:id="selectCategory" layoutX="316.0" layoutY="255.0" prefWidth="150.0" />
      <Button fx:id="startStreamNow" layoutX="341.0" layoutY="360.0" mnemonicParsing="false" onAction="#streamPage" style="-fx-background-color: Maroon;" text="Start Streaming!" textFill="#fcf0f0">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="goingBackOption" layoutX="706.0" layoutY="460.0" mnemonicParsing="false" onAction="#gotothePreviousPage" prefHeight="32.0" prefWidth="81.0" text="Go Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
